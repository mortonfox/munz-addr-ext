{
    "manifest_version": 3,
    "name": "Munz Addr",
    "version": "0.0.3",

    "description": "Reverse geocode a munzee and add its address to the page",

    "action": {
	"default_title": "munz_addr"
    },

    "permissions": [
        "storage"
    ],

    "host_permissions": [
        "https://dev.virtualearth.net/*"
    ],

    "options_ui": {
        "page": "options.htm",
        "open_in_tab": false
    },

    "content_scripts": [
	{
	    "matches": [
		"https://www.munzee.com/m/*"
	    ],
	    "js": ["main.js"]
	}
    ]
}
